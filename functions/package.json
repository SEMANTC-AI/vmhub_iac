{
  "name": "vmhub-provisioner",
  "version": "1.0.0",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
